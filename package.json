    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.20.0",
    "eslint-config-rational": "^0.1.24",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-check-updates": "^16.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
